----------------------------
| Conversion .txt -> .csv: |
----------------------------
cat 00.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 00.csv ; cat 01.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 01.csv ; cat 02.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 02.csv ; cat 03.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 03.csv ; cat 04.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 04.csv ; cat 05.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 05.csv ; cat 06.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 06.csv ; cat 07.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 07.csv ; cat 08.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 08.csv ; cat 09.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 09.csv ; cat 10.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 10.csv ; cat 11.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 11.csv ; cat 12.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 12.csv ; cat 13.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 13.csv ; cat 14.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 14.csv ; cat 15.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 15.csv ; cat 16.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 16.csv ; cat 17.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 17.csv ; cat 18.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 18.csv ; cat 19.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 19.csv ; cat 20.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 20.csv ; cat 21.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 21.csv ; cat 22.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 22.csv ; cat 23.txt | grep "^|.*" | sed -e 's/\s*,\s*/;/g' -e 's/\s*|\s*/,/g' -e 's/^,\|,$//g' -e 's/NULL//g' | awk '!seen[$0]++' >> 23.csv

grep -rl "group_concat(cast(serials.item as string);';')" *.csv | xargs sed -i "s/group_concat(cast(serials.item as string);';')/sensors/g"
